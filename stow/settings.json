{
  "permissions": {
    "defaultMode": "default",
    "additionalDirectories": []
  },
  "hooks": {},
  "statusLine": {
    "type": "command",
    "command": "~/.claude/ccline/ccline -t gruvbox",
    "padding": 0
  },
  "enabledPlugins": {
    "typescript-lsp@claude-plugins-official": true,
    "pyright-lsp@claude-plugins-official": true,
    "lua-lsp@claude-plugins-official": true,
    "frontend-design@claude-plugins-official": true,
    "claude-opus-4-5-migration@local": true,
    "base@local": true,
    "tool-proxy-mcp@tool-proxy": true,
    "tool-proxy-hooks@tool-proxy": true,
    "games@local": true
  },
  "alwaysThinkingEnabled": true,
  "preferences": {
    "autoCompact": true,
    "compactThreshold": 50000
  }
}
